{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server.js" },
    { "src": "/game", "dest": "server.js" },
    { "src": "/vendor/(.*)", "dest": "public/vendor/$1" },
    { "src": "/(.*)", "dest": "server.js" }
  ]
}